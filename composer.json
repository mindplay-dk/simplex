{
    "name": "mnapoli/simplex",
    "description": "Pimple fork with full container-interop support",
    "keywords": ["dependency injection", "container"],
    "homepage": "https://github.com/mnapoli/simplex",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Matthieu Napoli",
            "email": "matthieu@mnapoli.fr"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mindplay-dk/provider-interop.git"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "container-interop/container-interop": "^1.0",
        "mindplay/provider-interop": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": { "Simplex": "src/" }
    }
}
